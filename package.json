{
    "name": "@metalcamp/media-curator",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": false,
    "scripts": {
        "build": "npx tsc",
        "clean": "rimraf ./dist",
        "start": "ts-node src/index.ts",
        "test": "jest",
        "unit": "jest --config jest.unit.config.js",
        "integration": "jest --config jest.integration.config.js --detectOpenHandles",
        "typeorm": "ts-node -r tsconfig-paths/register --require dotenv/config $(yarn bin typeorm)",
        "migration:create": "yarn run typeorm migration:create -n",
        "migration:generate": "yarn run typeorm migration:generate -n",
        "migration:run": "yarn run typeorm migration:run",
        "migration:revert": "yarn run typeorm migration:revert"
    },
    "devDependencies": {
        "@types/jest": "^26.0.0",
        "@types/node": "^14.15.2",
        "@typescript-eslint/eslint-plugin": "^2.32.0",
        "@typescript-eslint/parser": "^2.32.0",
        "eslint": "^7.27.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.10.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^26.0.23",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "@fast-csv/parse": "^4.3.6",
        "@types/express": "^4.17.6",
        "@types/ioredis": "^4.26.4",
        "dotenv": "^8.6.0",
        "express": "^4.17.1",
        "express-winston": "^4.0.3",
        "fs": "^0.0.1-security",
        "ioredis": "^4.27.3",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.32",
        "winston": "^3.2.1"
    }
}
